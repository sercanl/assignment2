<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="no.uio.inf5750.assignment2.model.Degree" table="degree">

    <id name="id" column="id">
      <generator class="native"/>
    </id>
    
    <property name="type" column="type" type="string" not-null="true"/>
    <property name="requiredCourses" column="requiredCourses"/>
    <property name="degrees" column="degrees"/>
    
  </class>
  
</hibernate-mapping>
